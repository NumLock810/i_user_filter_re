{
	"manifest_version": 2,
	"name": "iwara user filter",
	"version": "0.1",
	"content_scripts": [
		{
			"matches": ["http://*.iwara.tv/videos*"],
			"js": ["main.js"],
			"css": ["style.css"],
			"run_at": "document_end"
		}
	],
	"options_ui":{
		"page": "options.html",
		"chrome_style": true
	},
	"permissions": ["storage"]

}

{
    "manifest_version": 3,
    "name": "iwara user filter:Re",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*.iwara.tv/videos*",
                "*://*.iwara.tv/images*"
            ],
            "js": [
                "main.js"
            ],
            "css": [
                "style.css"
            ],
            "run_at": "document_end"
        }
    ],
    "options_ui":{
        "page": "options.html",
        "open_in_tab": false
    },
    "permissions": [
        "storage"
    ]
    
}
